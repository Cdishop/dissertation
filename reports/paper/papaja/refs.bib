  author = {Booth, Jonathan E. and link will open in a new window Link to external site, this and Park, Tae-Youn and Zhu, Luke (Lei) and Beauregard, T. Alexandra and link will open in a new window Link to external site, this and Gu, Fan and Emery, Cécile},
