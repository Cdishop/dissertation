@article{newton_taking_2020,
  title = {Taking Engagement to Task: {{The}} Nature and Functioning of Task Engagement across Transitions.},
  shorttitle = {Taking Engagement to Task},
  author = {Newton, Daniel W. and LePine, Jeffery A. and Kim, Ji Koung and Wellman, Ned and Bush, John T.},
  date = {2020-01},
  journaltitle = {Journal of Applied Psychology},
  shortjournal = {Journal of Applied Psychology},
  volume = {105},
