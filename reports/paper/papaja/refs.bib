@article{weekley_low-fidelity_2015,
  title = {Low-{{Fidelity Simulations}}},
  author = {Weekley, Jeff A. and Hawkes, Ben and Guenole, Nigel and Ployhart, Robert E.},
  date = {2015-04-10},
  journaltitle = {Annual Review of Organizational Psychology and Organizational Behavior},
  shortjournal = {Annu. Rev. Organ. Psychol. Organ. Behav.},
  volume = {2},
  pages = {295--322},
  issn = {2327-0608},
  doi = {10.1146/annurev-orgpsych-032414-111304},
  url = {https://www-annualreviews-org.proxy1.cl.msu.edu/doi/10.1146/annurev-orgpsych-032414-111304},
  urldate = {2019-01-08},
  abstract = {Low-fidelity simulations, which combine closed-ended response options with realistic depictions of key job tasks, have grown in popularity for several practical reasons. Research into low-fidelity simulations, which has focused primarily on text-based situational judgment tests (SJTs), has shown that such measures (a) can predict a wide range of criteria, doing so with moderate adverse impact, depending on the construct saturation of the items, (b) are no more susceptible to coaching/cheating/retesting effects than many other forms of assessment and far less susceptible than some, and (c) continue to struggle when used to measure a single construct. Practice, by contrast, has moved well beyond text-based SJTs to include multimedia SJTs, online assessment center exercises, and game-like assessments. Because these are perceived more favorably by applicants, the same research dedicated to text-based SJTs is needed to guide development of these newer forms of low-fidelity simulation.},
  file = {/Users/christopherdishop/Zotero/storage/GCQ4GK42/Weekley et al. - 2015 - Low-Fidelity Simulations.pdf;/Users/christopherdishop/Zotero/storage/WMYVRA66/annurev-orgpsych-032414-111304.html},
  number = {1}
}

