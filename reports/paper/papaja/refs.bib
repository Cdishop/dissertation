@thesis{lin_relationship_2001,
  title = {The Relationship between Collective Efficacy and Task Interdependence},
  author = {Lin, Shu-Jiao},
  date = {2001},
  institution = {{University of Southern California}},
  location = {{United States -- California}},
  url = {http://search.proquest.com/pqdtglobal/docview/304722094/abstract/A81366660DB14CBDPQ/1},
  urldate = {2019-07-01},
  abstract = {The purpose of this study is to examine the hypothesis that collective efficacy and self-efficacy influence both the amount of effort expended by members of a group and the performance of the group under both high and low levels of task interdependence. Additionally, to improve our understanding of collective efficacy, two determinants of collective efficacy are hypothesized and examined: perceived adequacy of members' skills and perceived group coordination capability.
The study revolves around an experiment involving 768 college students that was conducted in Taiwan. To examine the hypothesis, a total of 256 three-person groups were assigned randomly to a task of either a high or low degree of interdependence (dealing with interdependence in operational teams). As a result, 112 high interdependence groups and 123 low interdependence groups were analyzed. Before and after the task, the participants were required to complete questionnaires to assess self-efficacy, collective efficacy, effort expenditure, and task interdependence.
Overall, at the group level of analysis, the results indicate that collective efficacy predicts the level of group effort invested and group performance. Contrary to expectations, collective efficacy has a significant effect on group effort only when subjects perform at a low level of interdependence. It also was discovered that the aggregate of individual group members' self-efficacy was not predictive of group effort in this study. Next, the perceived adequacy of group members' technical skills is a stronger determinant of collective efficacy than perceived group coordination capability regardless of the degree of task interdependence. In addition, self-efficacy is highly correlated with collective efficacy in tasks of both high and low interdependence. Finally, following Bandura (1997), collective efficacy has both individual and group level components. This study suggests that group members' perceived self-efficacy, perceived adequacy of group members' technical ability, and perceived group coordination capability all may contribute to a better determination of collective efficacy.
Limitations of the study are discussed, and possible avenues for future research as well as implications are addressed.},
  file = {/Users/christopherdishop/Zotero/storage/RFE8H7IZ/Lin - 2001 - The relationship between collective efficacy and t.pdf},
  keywords = {Collective efficacy,Education,Psychology,Self-efficacy,Task interdependence},
  langid = {english},
  pagetotal = {156},
  type = {Ph.D.}
}

